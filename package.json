{
  "name": "react-dd-menu",
  "version": "2.0.3",
  "description": "A react dropdown menu.",
  "main": "dist/index.js",
  "scripts": {
    "scripts": "babel ./src/js --out-dir ./dist",
    "styles": "sass --style expanded --source-map ./src/scss/react-dd-menu.scss ./dist/react-dd-menu.css && sass --style compressed ./src/scss/react-dd-menu.scss ./dist/react-dd-menu.min.css",
    "prebuild": "rimraf dist",
    "build": "npm run scripts && npm run styles",
    "postbuild": "postcss dist/*.css --use autoprefixer -d dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-dd-menu.git"
  },
  "keywords": [
    "react",
    "react-component",
    "dropown",
    "dropdown-menu",
    "component"
  ],
  "author": "mlaursen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-dd-menu/issues"
  },
  "homepage": "https://github.com/mlaursen/react-dd-menu#readme",
  "dependencies": {
    "classnames": "^2.3.2",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "react": ">= 15.3.0",
    "react-dom": ">= 15.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss-cli": "^10.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "sass": "^1.81.0"
  }
}
